{
  "name": "tsd",
  "version": "0.6.0-beta.4",
  "description": "The package manager for TypeScript definitions",
  "main": "dist/tsd.js",
  "bin": {
    "tsd": "dist/bin/tsd.js"
  },
  "preferGlobal": true,
  "files": [
    "dist/",
    "tsconfig.json",
    "LICENSE"
  ],
  "scripts": {
    "lint": "# TODO",
    "build": "rm -rf dist/ && tsc --project src",
    "test-spec": "ts-node --project src node_modules/blue-tape/bin/blue-tape.js src/**/*.spec.ts",
    "test-cov": "ts-node --project src node_modules/istanbul/lib/cli.js cover -e .ts --print none -x \"**/*.spec.ts\" blue-tape -- src/**/*.spec.ts",
    "test": "npm run lint && npm run test-cov",
    "bump-version": "FILE=\"src/bin/tsd.ts\"; if [ \"$(git status --porcelain | grep \"^ M \"$FILE)\" = \"\" ]; then VERSION=$(node -p \"require('./package.json').version\") && sed \"s/VERSION = '\\([0-9]*\\.[0-9]*\\.[0-9]*\\)'/VERSION = '\"$VERSION\"'/\" $FILE > .tmp.ts && mv .tmp.ts $FILE && git add $FILE; fi",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "bump-version",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/DefinitelyTyped/tsd.git"
  },
  "keywords": [
    "typescript",
    "definition",
    "package",
    "manager"
  ],
  "author": "Blake Embrey <hello@blakeembrey.com>",
  "contributors": [
    "Diullei Gomes (https://github.com/Diullei)",
    "Bart van der Schoor (https://github.com/Bartvds)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DefinitelyTyped/tsd/issues"
  },
  "homepage": "https://github.com/DefinitelyTyped/tsd",
  "devDependencies": {
    "blue-tape": "^0.1.10",
    "faucet": "0.0.1",
    "istanbul": "blakeembrey/istanbul#transpiler-support",
    "pre-commit": "^1.0.6",
    "ts-node": "^0.2.0"
  },
  "dependencies": {
    "arrify": "^1.0.0",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "es6-promise": "^3.0.2",
    "graceful-fs": "^4.1.2",
    "invariant": "^2.0.0",
    "mkdirp": "^0.5.1",
    "native-or-bluebird": "^1.2.0",
    "parse-json": "^2.2.0",
    "popsicle": "^1.1.0",
    "popsicle-cache": "0.0.2",
    "semver": "^5.0.1",
    "sort-keys": "^1.0.0",
    "strip-bom": "^2.0.0",
    "thenify": "^3.1.0",
    "thenify-all": "^1.6.0",
    "typescript": "^1.6.0-beta",
    "util-partial": "^1.0.0",
    "xtend": "^4.0.0",
    "zip-object": "^0.1.0"
  }
}
