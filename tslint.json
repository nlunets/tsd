{
  "rules": {
    "ban": [],
    "class-name": true,
    "comment-format": [
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": 2,
    "interface-name": false,
    "jsdoc-format": true,
    "label-position": true,
    "label-undefined": true,
    "max-line-length": 80,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": false,
    "no-construct": true,
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-string-literal": true,
    "no-trailing-whitespace": true,
    "no-unreachable": true,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "one-line": [
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": "single",
    "radix": true,
    "semicolon": false,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": true,
    "typedef-whitespace": false,
    "use-strict": false,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-separator",
      "check-operator",
      "check-type",
      "check-typecast"
    ]
  }
}
